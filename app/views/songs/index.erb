<h1>Music Library</h1>
<table>

    <thead>
        <tr>
            <th>Title</th>
            <th>Artist name</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
            </tr>
        <% end %>
    </tbody>

</table>