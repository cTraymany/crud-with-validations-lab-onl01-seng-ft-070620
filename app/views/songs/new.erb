<h1>Create song</h1>
<%= form_for @song do |f| %>
    <table>
        <tr>
            <td>
                <%= f.label :title %><br>
                <%= f.text_field :title %><br><br>
            </td>
        </tr>
        
        <tr>
            <td>
                <%= f.label :release_year %><br>
                <%= f.text_field :release_year %><br><br>
            </td>
        </tr>

        <tr>
            <td>
                <%= f.label :released %><br>
                <%= f.check_box :released %><br><br>
            </td>
        </tr>

        <tr>
            <td>
                <%= f.label :genre %><br>
                <%= f.text_field :genre %><br><br>
            </td>
        </tr>

        <tr>
            <td>
                <%= f.label :artist_name %><br>
                <%= f.text_field :artist_name %><br><br>
            </td>
        </tr>


        <tr>
            <td><%= f.submit %></td>
        </tr>
    </table>
<% end %>
